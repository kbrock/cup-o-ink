<h1 class="page-header"><%= printer.name %></h1>
<dl>
  <dt>Model</dt>
  <dd><%= printer.info %></dd>
  <dt>Location</dt>
  <dd><%= printer.location %></dd>
</dl>
<% if printer.toners.present? %>
<table class="table table-striped table-condensed">
 <thead>
  <tr>
    <td>%</td><td>Color</td><td>Toner name</td><td>Type</td>
  </tr>
</thead>
<tbody>
<%= render printer.toners %>
</tbody>
</table>
<% end %>
<hr />
